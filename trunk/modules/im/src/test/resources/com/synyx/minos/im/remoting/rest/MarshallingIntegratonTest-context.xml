<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<import resource="classpath*:com/synyx/minos/**/rest-context.xml" />

	<bean id="userManagement" class="org.mockito.Mockito" factory-method="mock">
		<constructor-arg value="org.synyx.minos.umt.service.UserManagement" />
	</bean>
	
	<bean id="instantMessagingService" class="org.mockito.Mockito" factory-method="mock">
		<constructor-arg value="org.synyx.minos.im.service.InstantMessagingService" />
	</bean>

</beans>
