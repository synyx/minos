<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
	
	<bean class="org.synyx.minos.cmt.service.ContactItemManagementImpl" name="contactItemManagement">
		<property name="contactItemDao" ref="contactItemDao" />
	</bean>
	<bean class="org.synyx.minos.cmt.service.PersonManagementImpl" name="personManagement">
		<property name="personDao" ref="personDao" />
	</bean>
	<bean class="org.synyx.minos.cmt.service.OrganisationManagementImpl" name="organisationManagement">
		<property name="organisationDao" ref="organisationDao" />
	</bean>
	<bean class="org.synyx.minos.cmt.service.ContactManagementImpl" name="contactManagement">
		<property name="contactDao" ref="contactDao" />
	</bean>
	<bean class="org.synyx.minos.cmt.service.AddressManagementImpl" name="addressManagement">
		<property name="addressDao" ref="addressDao"/>
	</bean>
		
</beans>
